{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: vitest",
      "request": "launch",
      "runtimeArgs": ["run-script", "test"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug: chrome (localhost)",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}
